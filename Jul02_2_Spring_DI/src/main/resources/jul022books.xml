<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	
	
	
	<bean id="b1" class="com.oj.jul022.book.Book">
		<property name="name" value="실전에서 바로쓰는 Next.js"></property>
		<property name="price" value="35000"></property>
	</bean>
	<bean id="b2" class="com.oj.jul022.book.Book">
		<constructor-arg value="혼자공부하는자바"></constructor-arg>
		<constructor-arg value="40000"></constructor-arg>
	</bean>
</beans>
