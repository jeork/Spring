<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oj.jun082.fruit.FruitMapper">

	<select id="searchFruit" resultType="com.oj.jul082.fruit.Fruit" parameterType="com.oj.jul082.fruit.Fruit"> 
		select * from jul08_fruit where f_price &lt;= #{f_price} order by f_price  
		
	</select>
	
</mapper>